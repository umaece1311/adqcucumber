{
  "name": "cypress",
  "version": "1.0.0",
  "description": "E2E tests",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest:ci": "yarn run clean:reports",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "send-reports-ci": "yarn run combine-reports && yarn run generate-report",
    "prettier-all-app": "yarn prettier --write \"**/*.js\"",
    "core:smoke":"yarn cypress run -e TAGS=@smoke --browser chrome --spec cypress/e2e/Cucumber/features/**/**/**/*.feature",
    "core:regression":"yarn cypress run -e TAGS=@regression --browser chrome --spec cypress/e2e/Cucumber/features/**/**/**/*.feature",
    "core:sidebar":"yarn cypress run -e TAGS=@sidebar --browser chrome --spec cypress/e2e/Cucumber/features/**/**/**/*.feature",
    "notify-slack": "yarn cypress-slack-reporter --report-dir 'cypress/reports/mochareports'",
    "effective:config": "circleci config process config.yml"
  },
  "author": "uma",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "11.3.1",
    "@bahmutov/cy-api": "^1.5.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@cypress/webpack-preprocessor": "^5.4.11",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@slack/types": "^1.10.0",
    "@slack/webhook": "^5.0.4",
    "axe-core": "^4.1.1",
    "cy-verify-downloads": "^0.1.11",
    "cypress": "12.1.0",
    "cypress-axe": "^0.12.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-iframe": "^1.0.1",
    "cypress-image-snapshot": "^4.0.0",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-promise": "^1.1.0",
    "cypress-real-events": "^1.7.6",
    "cypress-recurse": "^1.29.0",
    "cypress-slack-reporter": "1.5.0",
    "cypress-time-marks": "^1.4.0",
    "cypress-wait-until": "1.7.1",
    "cypress-xpath": "^2.0.1",
    "esbuild": "^0.14.48",
    "eslint": "^7.18.0",
    "eslint-plugin-cypress": "^2.11.2",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "prettier": "^2.1.2",
    "socket.io": "4.2.0",
    "socket.io-client": "4.2.0",
    "typescript": "^4.4.4"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/Cucumber/**/*.{js,ts}"
  },
  "dependencies": {
    "cypress-grep": "^2.13.1",
    "cypress-plugin-steps": "^1.0.4",
    "cypress-select-tests": "^1.7.1",
    "cypress-tags": "0.3.0",
    "increase-memory-limit": "^1.0.7",
    "jsonpath": "^1.1.1",
    "xlsx": "^0.18.5"
  }
}
